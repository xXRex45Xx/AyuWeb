<% if(err.statusCode===404){ %>

    <head>
        <%- include("../head.ejs") %>
            <link rel="stylesheet" href="/css/PatientPage/PatientPage.css">
    </head>

    <body>
        <div class="wrapper">
            <%- include("mainnaverr.ejs") %>
                <div class="mainContainer h-100">
                    <% } %>

                        <div class="row justify-content-center align-items-start">
                            <div class="alert alert-danger col-6 mt-5" role="alert">
                                <h4 class="alert-heading">Error - <%= err.statusCode %>
                                </h4>
                                <h4 class="alert-heading">
                                    <%= err.message %>
                                </h4>
                                <% if(process.env.NODE_ENV !== "production"){ %> 
                                <p>
                                    <%= err.stack %>
                                </p>
                                <% } %> 
                            </div>
                        </div>
                        <% if(err.statusCode===404){ %>
                </div>
        </div>
    </body>
    <% } %>