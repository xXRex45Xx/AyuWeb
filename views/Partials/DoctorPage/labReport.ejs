                    <tbody>
                        <% recent.forEach(labreport => { %>
                            <tr>
                                <th><%= (labreport.DateTimeOfRequest).toString().slice(0,15) %></th>
                                <th><%= labreport.ReportType%></th>
                                <th><%= labreport.NormalValue%></th>
                                <th><%= labreport.Result%></th>
                            </tr>
                            <%})%>
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        
